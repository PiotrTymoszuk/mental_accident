doi = {10.1001/JAMAPSYCHIATRY.2021.2427},
file = {:C\:/Users/piotr/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ziobrowski et al. - 2021 - Development and Validation of a Model to Predict Posttraumatic Stress Disorder and Major Depression After a M.pdf:pdf},
