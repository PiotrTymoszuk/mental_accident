journal = {BMC psychiatry},
keywords = {Adolescent,Adult,Aged,Anne-Marie Schott,Clinical Trial,Cohort Studies,Cross-Sectional Studies,Diagnostic and Statistical Manual of Mental Disord,Emergency Service,Female,Follow-Up Studies,Hospital / trends*,Humans,MEDLINE,Male,Marie Viprey,Middle Aged,Mohammad-Hashim Wafa,Multicenter Study,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,Non-U.S. Gov't,PMC6543570,Patient Admission / trends*,Post-Traumatic / diagnosis*,Post-Traumatic / psychology*,Post-Traumatic / therapy,Predictive Value of Tests,Prospective Studies,PubMed Abstract,Research Support,Stress Disorders,Surveys and Questionnaires,Young Adult,doi:10.1186/s12888-019-2154-z,pmid:31146712},
