keywords = {Asia / epidemiology,Emergency Responders / psychology,Emergency Responders / statistics & numerical data,Europe / epidemiology,Evandro Silva Freire Coutinho,Extramural,Humans,MEDLINE,Mauro Vitor Mendlowicz,Meta-Analysis,N.I.H.,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,Non-U.S. Gov't,North America / epidemiology,Occupational Diseases / epidemiology*,PMC3974968,Population Surveillance,Post-Traumatic / epidemiology*,Prevalence,PubMed Abstract,Rescue Work*,Research Support,Review,Stress Disorders,Systematic Review,William Berger,doi:10.1007/s00127-011-0408-2,pmid:21681455},
month = {jun},
