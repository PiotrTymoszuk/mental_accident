keywords = {20th Century,21st Century,Andrea Burri,Andreas Maercker,Crime Victims / psychology*,Disaster Victims / psychology*,Europe / epidemiology,Historical Article,History,Humans,MEDLINE,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,PMC4114166,Post-Traumatic / epidemiology,Post-Traumatic / psychology*,Prevalence,PubMed Abstract,Social Values / history*,Stress Disorders,Value of Life*,World War II,doi:10.1186/1756-0500-7-407,pmid:24972489},
